<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="WARN">
<Properties>
<Property name="basePath">resources/</Property>
</Properties>
<Appenders>
	<RollingFile name="generateLogs" fileName="${basePath}/log4j2.xml" filePattern="${basePath}/reporting-%d{yyyy-MM-dd}.log">
		<PatternLayout pattern="%d{hh:mm:ss} [%t] [%-5level] %m%n"/>
		<SizebasedTriggeringPolicy size="500"/>
	</RollingFile>
</Appenders>
	<Loggers>
	<Logger name="fileLogs" level="trace" additivity="false">
		<AppenderRef ref="generateLogs"/>
	</Logger>
		<Root level="trace">
			<AppenderRef ref="generateLogs"/>
		</Root>
	</Loggers>
</Configuration> 